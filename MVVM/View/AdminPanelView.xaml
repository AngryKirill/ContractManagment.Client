<UserControl x:Class="ContractManagment.Client.MVVM.View.AdminPanelView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ContractManagment.Client.MVVM.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <RadioButton Content="Ключи"
                         Style="{StaticResource MenuButtonTheme}"/>
            <RadioButton Content="Документы"
                         Style="{StaticResource MenuButtonTheme}"/>
            <RadioButton Content="Пользователи"
                         Style="{StaticResource MenuButtonTheme}"/>
            <RadioButton Content="История"
                         Style="{StaticResource MenuButtonTheme}"/>
            <RadioButton Content="Заявки"
                         Style="{StaticResource MenuButtonTheme}"/>
            <RadioButton Content="Клиенты"
                         Style="{StaticResource MenuButtonTheme}"/>
        </StackPanel>
    </Grid>
</UserControl>
